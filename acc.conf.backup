## This is comment

## Maxim 200 lines are allowed per page
## 23 are the reccomended default
## use 0 to disable
lines_per_page = 23

## used for dates: 18/3/2007
date_separator_char =   "/"

## determine what date format to use
## 1(true) for ISO and 0(false) for USA format
use_iso_date_format = 1

## mathematical precision of the currency values
## the default is 2 digits
value_precision = 2

## Eport paths max 1000 chars
export_all_path= export_all.txt
export_income_path= export_income.txt
export_outcome_path= export_outcome.txt

## If you want the program to set new ID numbers each 
## time it starts set this property to 1(true) else to 0
set_new_IDs_on_appload = 0

## Property used to store the current DB path
last_database_used = test5.adb

last_filter_expression = 

showlist:remember_last_shown = 0
showfilteredlist:remember_last_shown = 0

export_max_width = 40;

